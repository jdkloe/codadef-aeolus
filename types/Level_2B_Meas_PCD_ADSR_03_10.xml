              </cd:Integer>
            </cd:Field>
            <cd:Field name="spare">
              <cd:Raw>
                <cd:Description>Spare</cd:Description>
                <cd:BitSize>8</cd:BitSize>
              </cd:Raw>
              <cd:Hidden/>
            </cd:Field>
          </cd:Record>
        </cd:Array>
      </cd:Field>
      <cd:Field name="spare">
        <cd:Raw>
          <cd:Description>Spare</cd:Description>
