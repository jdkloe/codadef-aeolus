<?xml version="1.0"?>
<cd:Record xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10" format="binary" last-modified="2019-10-31" name="Level_2A_ICA_Opt_MDSR_03_02">
  <cd:Field name="starttime">
    <cd:Time timeformat="binary_envisat_datetime">
      <cd:Description>Start date and time of the ICA profile covered by the present DSR</cd:Description>
      <ct:NamedTest id="RangeSecond" path="seconds"/>
      <ct:NamedTest id="RangeMicroSecond" path="microseconds"/>
    </cd:Time>
  </cd:Field>
  <cd:Field name="ica_optical_properties">
    <cd:Array>
      <cd:Dimension>24</cd:Dimension>
      <cd:Record>
        <cd:Description>Structure containing ICA products</cd:Description>
        <cd:Field name="case">
          <cd:Integer>
            <cd:Description>Filling case estimated by the algorithm</cd:Description>
            <cd:BitSize>8</cd:BitSize>
            <cd:NativeType>uint8</cd:NativeType>
          </cd:Integer>
        </cd:Field>
        <cd:Field name="extinction">
          <cd:Float>
            <cd:Description>Particle extinction coefficient of the bin. Missing value: -1e+06.</cd:Description>
            <cd:Unit>10^-6 m^-1</cd:Unit>
            <cd:BitSize>64</cd:BitSize>
            <cd:NativeType>double</cd:NativeType>
            <ct:NamedTest id="FloatIsFinite"/>
          </cd:Float>
        </cd:Field>
        <cd:Field name="backscatter">
          <cd:Float>
            <cd:Description>Particle backscatter coefficient of the bin. Missing value: -1e+06.</cd:Description>
            <cd:Unit>10^-6 sr^-1 m^-1</cd:Unit>
            <cd:BitSize>64</cd:BitSize>
            <cd:NativeType>double</cd:NativeType>
            <ct:NamedTest id="FloatIsFinite"/>
          </cd:Float>
        </cd:Field>
        <cd:Field name="lod">
          <cd:Float>
            <cd:Description>Particle LOD of the bin. Missing value: -1.</cd:Description>
            <cd:BitSize>64</cd:BitSize>
            <cd:NativeType>double</cd:NativeType>
            <ct:NamedTest id="FloatIsFinite"/>
          </cd:Float>
        </cd:Field>
      </cd:Record>
    </cd:Array>
  </cd:Field>
</cd:Record>
