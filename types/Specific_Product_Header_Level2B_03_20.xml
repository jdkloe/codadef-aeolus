      </cd:Field>
    </cd:Record>
  </cd:Field>
  <cd:Field name="List_of_Invalid_Rayleigh_Profile_Counts">
    <cd:Record namexml="List_of_Invalid_Rayleigh_Profile_Counts">
      <cd:Attribute name="count"/>
      <cd:Field name="Invalid_Rayleigh_Profile_Count">
        <cd:NamedType id="Level_2BC_Count_Type_XML_03_20"/>
      </cd:Field>
    </cd:Record>
  </cd:Field>
  <cd:Field name="Num_Profiles_Surface_Mie">
    <cd:Type namexml="Num_Profiles_Surface_Mie">
      <cd:Integer format="ascii">
        <cd:Description>Number of Mie profiles reported in L2B extending down to the surface</cd:Description>
        <cd:NativeType>uint16</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Profiles_Surface_Ray">
    <cd:Type namexml="Num_Profiles_Surface_Ray">
      <cd:Integer format="ascii">
        <cd:Description>Number of Rayleigh profiles reported in L2B extending down to the surface</cd:Description>
        <cd:NativeType>uint16</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="List_of_Valid_L2B_Mie_Wind_Counts">
    <cd:Record namexml="List_of_Valid_L2B_Mie_Wind_Counts">
      <cd:Attribute name="count"/>
      <cd:Field name="Valid_L2B_Mie_Wind_Count">
        <cd:NamedType id="Level_2BC_Count_Type_XML_03_20"/>
      </cd:Field>
    </cd:Record>
  </cd:Field>
  <cd:Field name="List_of_Valid_L2B_Rayleigh_Wind_Counts">
    <cd:Record namexml="List_of_Valid_L2B_Rayleigh_Wind_Counts">
      <cd:Attribute name="count"/>
      <cd:Field name="Valid_L2B_Rayleigh_Wind_Count">
        <cd:NamedType id="Level_2BC_Count_Type_XML_03_20"/>
      </cd:Field>
    </cd:Record>
  </cd:Field>
  <cd:Field name="List_of_Invalid_L2B_Mie_Wind_Counts">
    <cd:Record namexml="List_of_Invalid_L2B_Mie_Wind_Counts">
      <cd:Attribute name="count"/>
      <cd:Field name="Invalid_L2B_Mie_Wind_Count">
        <cd:NamedType id="Level_2BC_Count_Type_XML_03_20"/>
      </cd:Field>
    </cd:Record>
  </cd:Field>
  <cd:Field name="List_of_Invalid_L2B_Rayleigh_Wind_Counts">
    <cd:Record namexml="List_of_Invalid_L2B_Rayleigh_Wind_Counts">
      <cd:Attribute name="count"/>
      <cd:Field name="Invalid_L2B_Rayleigh_Wind_Count">
        <cd:NamedType id="Level_2BC_Count_Type_XML_03_20"/>
      </cd:Field>
    </cd:Record>
  </cd:Field>
